package Main;
public class Main {
	
	static int lSearch(int[] array,int n, int x) {
		
		for(int i=0;i<n;i++) {
			
			if(array[i] == x) {
				return i;
				
			}
			
			
		}
		System.out.println("Element not found!!");

		
		return -1;
	}

	public static void main(String[] args) {
		
		int array[] = {10,20,30,40,50,60,70};
		int n = array.length;
		int x = 408;
		System.out.println(lSearch(array,n,x));

	}

}
